def sum_of_squares(N):
    """Return the sum of squares from 1^2 to N^2."""
    total_sum = 0
    for i in range(1, N + 1):
        total_sum += i * i
    return total_sum

# Sample input
N = 6

# Compute the sum of squares
sum_result = sum_of_squares(N)

# Print the result
print(f"Sum = {sum_result}")
